<% provide(:title, "New Question") %>
<h1>New Question</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for [:admin, @question] do |f| %>
      <%= f.label :content %>
      <%= f.text_field :content, class: 'form-control' %>
      <%= f.submit "Add", class: "btn btn-default pull-right" %>
    <% end %>
  </div>
</div>