<% provide(:title, "Edit Category") %>
<h1>Edit Category</h1>
<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for @category, :url => admin_questions_category_path do |f| %>
      <%= f.label :name %>
      <%= f.text_field :name, class: 'form-control' %>
      <%= f.submit "Save", class: "btn btn-default pull-right" %>
    <% end %>
  </div>
</div>