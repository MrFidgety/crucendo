<% provide(:title, "New Category") %>
<h1>New Category</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for @category, :url => admin_questions_categories_path do |f| %>
      <%= f.label :name %>
      <%= f.text_field :name, class: 'form-control' %>
      <%= f.submit "Add", class: "btn btn-default pull-right" %>
    <% end %>
  </div>
</div>